{
    "name": "codelicia/trineforce",
    "description": "Salesforce Soql Doctrine Driver",
    "homepage": "https://github.com/codelicia/",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandre Eher",
            "email": "alexandre@eher.com.br"
        },
        {
            "name": "Jefersson Nathan",
            "email": "malukenho.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1 || ^8.2",
        "ext-json": "*",
        "ext-pdo": "*",
        "azjezz/psl": "^2.7.0",
        "doctrine/dbal": "^3.6.4",
        "doctrine/orm": "^2.15.3",
        "guzzlehttp/guzzle": "^7.7.0",
        "guzzlehttp/psr7": "^2.5.0",
        "psr/http-message": "^1.1"
    },
    "require-dev": {
        "doctrine/coding-standard": "^12.0.0",
        "malukenho/mcbumpface": "^1.2.0",
        "phpunit/phpunit": "^10.2.6",
        "roave/security-advisories": "dev-master",
        "staabm/annotate-pull-request-from-checkstyle": "^1.8.5",
        "maglnet/composer-require-checker": "^4.6.0"
    },
    "autoload": {
        "psr-4": {
            "Codelicia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeliciaTest\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "malukenho/mcbumpface": true,
            "infection/extension-installer": true
        }
    },
    "scripts": {
        "phpcbf": "vendor/bin/phpcbf",
        "phpcs": "vendor/bin/phpcs",
        "phpunit": "php -dzend.assertions=1 vendor/bin/phpunit",
        "require-checker": "vendor/bin/composer-require-checker check composer.json",
        "test": [
            "@phpcs",
            "@phpunit",
            "@require-checker"
        ]
    }
}
