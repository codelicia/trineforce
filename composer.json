{
    "name": "codelicia/trineforce",
    "description": "Salesforce Soql Doctrine Driver",
    "homepage": "https://github.com/codelicia/",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandre Eher",
            "email": "alexandre@eher.com.br"
        },
        {
            "name": "Jefersson Nathan",
            "email": "malukenho.dev@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "ext-pdo": "*",
        "beberlei/assert": "^v3.3.2",
        "doctrine/dbal": "^3.5.1",
        "doctrine/orm": "^2.13.3",
        "guzzlehttp/guzzle": "^6.0 | ^7.0",
        "webmozart/assert": "^1.11.0"
    },
    "require-dev": {
        "doctrine/coding-standard": "^10.0.0",
        "malukenho/mcbumpface": "^1.1.5",
        "phpunit/phpunit": "^8.3 | ^9.0",
        "roave/security-advisories": "dev-master",
        "staabm/annotate-pull-request-from-checkstyle": "^1.8.3",
        "maglnet/composer-require-checker": "^4.2.0"
    },
    "autoload": {
        "psr-4": {
            "Codelicia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeliciaTest\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "malukenho/mcbumpface": true
        }
    }
}
