{
    "name": "codelicia/trineforce",
    "description": "Salesforce Soql Doctrine Driver",
    "homepage": "https://github.com/codelicia/",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandre Eher",
            "email": "alexandre@eher.com.br"
        },
        {
            "name": "Jefersson Nathan",
            "email": "malukenho.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1 || ^8.2",
        "ext-json": "*",
        "ext-pdo": "*",
        "azjezz/psl": "^2.4.1",
        "doctrine/dbal": "^3.6.1",
        "doctrine/orm": "^2.14.1",
        "guzzlehttp/guzzle": "^7.5.0",
        "guzzlehttp/psr7": "^2.4.3",
        "psr/http-message": "^1.0.1"
    },
    "require-dev": {
        "doctrine/coding-standard": "^11.1.0",
        "malukenho/mcbumpface": "^1.2.0",
        "phpunit/phpunit": "^10.0.14",
        "roave/security-advisories": "dev-master",
        "staabm/annotate-pull-request-from-checkstyle": "^1.8.3",
        "maglnet/composer-require-checker": "^4.5.0"
    },
    "autoload": {
        "psr-4": {
            "Codelicia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeliciaTest\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "malukenho/mcbumpface": true
        }
    }
}
